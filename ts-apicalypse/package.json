{
  "name": "@phalcode/ts-apicalypse",
  "type": "module",
  "version": "1.0.23",
  "description": "A Typescript client and request builder for Apicalypse",
  "author": "Phalcode <contact@phalco.de>",
  "keywords": [
    "apicalypse"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Phalcode/ts-igdb-client"
  },
  "homepage": "https://github.com/Phalcode/ts-igdb-client/tree/main/ts-apicalypse",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "axios": "^1.11.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "ts-toolbelt": "^9.6.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "clean": true,
    "dts": {
      "resolve": true
    }
  }
}
